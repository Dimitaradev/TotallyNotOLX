@model StatusCodeErrorViewModel
<link href="@Url.Content("~/css/Error/errorpage.css")" rel="stylesheet" type="text/css" />
@if (Model.ErrorCode == "404")
{
    <img src="https://th.bing.com/th/id/OIP.FuWXD5OAEc4mJwGp4swt0QHaEF?w=281&h=180&c=7&r=0&o=5&pid=1.7" alt="Alternate Text" class="center" />
}
else if (Model.ErrorCode == "403")
{
    <img src="data:image/png;base64," alt="Alternate Text" class="center" />
}
else if (Model.ErrorCode == "405")
{
    <img src="data:image/jpeg;base64," alt="Alternate Text" class="center" />
}
else if (Model.ErrorCode == "500")
{
    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSC1WQtoxrdTtHYFy0MfGbvqxSeZnGNMJjFmw&usqp=CAU" alt="Alternate Text" class="center" />
}
else if (Model.ErrorCode == "505")
{
    <h2 class="error_505">@Model.ErrorCode</h2>
}
<h2>@Model.ErrorDescription</h2>
<h2><a asp-controller="Product" asp-action="Index">Back to all items</a></h2>